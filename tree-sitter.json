{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "nginx",
      "camelcase": "Nginx",
      "title": "Nginx",
      "scope": "source.nginx",
      "file-types": [
        "nginx.conf",
        "conf"
      ],
      "injection-regex": "^nginx$",
      "class-name": "TreeSitterNginx"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Nginx grammar for tree-sitter",
    "authors": [
      {
        "name": "killeik",
        "email": "https://killeik.net",
        "url": "https://killeik.net/"
      }
    ],
    "links": {
      "repository": "https://github.com/killeik/tree-sitter-nginx"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}